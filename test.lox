class EmergenceSearcher {

    fun search (x) {
        if (x == nil or x > 177013) {
            print "Cannot search!";
            return;
        }

        var isFound = false;

        while (!isFound) {
            if (x == 177013) {
                isFound = true;
                return x;
            }

            x = x + 1.2;
        }
    }
}

var x = 10;
var emergenceSearcher = EmergenceSearcher();
var emergence = emergenceSearcher.search(x);
